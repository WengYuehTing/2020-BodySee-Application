<Window x:Class="BodySee.Windows.VolumeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BodySee.Windows"
        mc:Ignorable="d"
        WindowStyle="None"
        Topmost="true"
        Title="VolumeWindow" Height="109.362" Width="375.697" StateChanged="Window_StateChanged">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Image Margin="5" Source="../Images/音量.png"></Image>
        <Slider Margin="10" Name="VolumeSlider" IsSelectionRangeEnabled="True" Grid.Column="1" VerticalAlignment="Center" Style="{DynamicResource SliderStyle1}"></Slider>
    </Grid>
</Window>
